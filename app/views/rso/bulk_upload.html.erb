<div class="row">
  <div class="col-md-12">
    <% if flash[:success] %>
        <div class="alert alert-success"><%= flash[:success] %></div>
    <% end %>
    <% if flash[:error] %>
        <div class="alert alert-danger"><%= flash[:error] %></div>
    <% end %>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <h3>RSO Bulk Upload</h3>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <%= form_for(Csv.new, url: rso_bulk_upload_path, method: "post") do |f|%>
      <div class="form-group">
        <%= f.label :csvfile, "CSV File Upload" %>
        <%= f.file_field :csvfile %>
      </div>
      <%= f.submit "Upload", class: "btn btn-primary btn-block"%>
    <% end %>
  </div>
</div>
