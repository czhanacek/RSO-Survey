<div class="row">
  <div class="col-md-12">
    <h3>Registered Organizations</h3>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
      <table class="table table-striped table-hover">
        <thead class="table-danger">
          <th width=30%>Name</th>
          <th width=30%>Nickname</th>
          <th width=30%>Website</th>
          <th width=5%>Keywords</th>
          <th width=5%></th>
        </thead>
        <tbody>
          <% @rsos.each do |r| %>
            <%= render partial: "rso", :locals => {:r => r} %>
          <% end %>
        </tbody>
      </table>
  </div>
</div>
